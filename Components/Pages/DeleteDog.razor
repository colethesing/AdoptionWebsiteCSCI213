@page "/DeleteDog"
@using System.Text;
@using AdoptionWebsiteTest.Models;
@using Microsoft.Data.SqlClient;
@using System;
@using System.Data;
@using System.Data.SqlClient;
@using System.Collections;

<h3>Delete This Dog</h3>

<form>
    <label for="DogName">Name:</label>
    <input type="text" id="DogName" name="DogName">

  
    <br>
    <label for="DogAge">Age:</label>
    <input type="text" id="DogAge" name="DogAge">
    
    <br>
    <label for="DogBreed">Breed:</label>
    <input type="text" id="DogBreed" name="DogBreed">
    
    <br>
    <label for="DogDescription">Description:</label>
    <input type="text" id="DogDescription" name="DogDescription">

    <br>
    <label for="DogPhoto">Photo</label>
    <input type="file" id="DogPhoto" name="DogPhoto" accept="image/png", image/jpeg />
</form>